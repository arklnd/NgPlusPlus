{
    "name": "hyland-ui",
    "version": "9.0.4",
    "scripts": {
        "ng": "ng",
        "cypress": "cypress",
        "start:dev-app": "ng run ui-dev:serve --host 0.0.0.0 --disable-host-check",
        "start:storybook": "npm run compodoc && ng run storybook:storybook",
        "start:storybook:packages": "npm run simulate-packages && npm run compodoc && ng run storybook:storybook --ts-config ./.storybook/tsconfig.packages.json",
        "serve-storybook": "angular-http-server --path ./dist/storybook -p 6006 --silent",
        "serve-dev-app": "wait-on ./dist/ui-dev/index.html -v -t 2000 && angular-http-server --path ./dist/ui-dev -p 4200",
        "build:ui": "npm run :ci:build:projects",
        "build:dev-app": "ng build ui-dev",
        "build:dev-app-prod": "ng build ui-dev --configuration production",
        "build:storybook": "npm run compodoc && ng run storybook:build-storybook:ci",
        "build:storybook:packages": "npm run simulate-packages && npm run compodoc && ng run storybook:build-storybook:ci --ts-config ./.storybook/tsconfig.packages.json",
        "lint:projects": "npm-run-all lint:project:**",
        "lint:project:ui": "ng lint @hyland/ui",
        "lint:project:ui-shell": "ng lint @hyland/ui-shell",
        "lint:project:ui-infragistics": "ng lint @hyland/ui-infragistics",
        "lint:project:dev-app": "ng lint ui-dev",
        "lint:project:e2e": "ng lint e2e",
        "pretty-quick": "pretty-quick && pretty-quick --staged",
        "analyze-bundle": "source-map-explorer dist/ui-dev/*.js",
        "update-changelogs": "node tools/build/release/update-changelogs.js",
        "prepare-dev-app-prod-files": "sh ./projects/ui-dev/tools/build-production-dev-app.sh",
        "install:infragistics-prod": "node ./tools/build/infragistics-package-update.js prod",
        "install:infragistics-dev": "node ./tools/build/infragistics-package-update.js dev",
        "cy:open": "cypress open -C cypress/cypress.base.config.ts --browser chrome",
        "cy:open:a11y": "cypress open -C cypress/cypress.a11y.config.ts --e2e --browser chrome",
        "cy:run:storybook": "cypress run -C cypress/cypress.storybook.config.ts --browser chrome",
        "cy:run:dev-app": "wait-on http://localhost:4200 -v -t 5000 && cypress run -C cypress/cypress.dev-app.config.ts --browser chrome",
        "cy:run:a11y": "cypress run -C cypress/cypress.a11y.config.ts --browser chrome",
        "run:a11y:generate": "node a11y/generateStoriesURLs.js",
        "cy:run:component": "cypress run -C cypress/cypress.component.config.ts --browser chrome --component --quiet",
        "cy:run:filtered": "node cypress/filter-specs.js",
        "cy:component:coverage": "wait-on ./cypress/.nyc_output/out.json -v -t 5000 && npx nyc report --reporter=text-summary --temp-dir=./cypress/.nyc_output",
        "visual-regression:test:low-res": "node ./backstop_data/backstop-generator.js && node node_modules/backstopjs/cli/index.js test --config=backstop_data/backstop.json --docker",
        "visual-regression:test:iphone": "node ./backstop_data/backstop-generator.js --configFile=./configs/mobile.config.json && node node_modules/backstopjs/cli/index.js test --config=backstop_data/backstop.json --docker",
        "visual-regression:test:high-res": "node ./backstop_data/backstop-generator.js --configFile=./configs/high-res.config.json && node node_modules/backstopjs/cli/index.js test --config=backstop_data/backstop.json --docker",
        "visual-regression:test:dark-theme": "node ./backstop_data/backstop-generator.js --configFile=./configs/dark-theme-low-res.config.json && node node_modules/backstopjs/cli/index.js test --config=backstop_data/backstop.json --docker",
        "visual-regression:approve": "node node_modules/backstopjs/cli/index.js approve --config=./backstop_data/backstop.json --docker",
        "visual-regression:test-approve:all": "node ./backstop_data/backstop-run-all.js",
        ":build:project:ui": "ng build --project @hyland/ui --configuration production && node -e \"require('fs').copyFileSync('dist/hyland/ui/i18n/en.i18n.json', 'dist/hyland/ui/i18n/en-us.i18n.json')\"",
        ":build:project:ui:storybook": "tsc -p ./.storybook/common/tsconfig.lib.json && cpx \".storybook/common/{package.json,**/*.mdx}\" dist/hyland/ui/storybook",
        ":build:project:ui-shell": "ng build --project @hyland/ui-shell --configuration production && node -e \"require('fs').copyFileSync('dist/hyland/ui-shell/src/i18n/en.i18n.json', 'dist/hyland/ui-shell/src/i18n/en-us.i18n.json')\"",
        ":build:project:ui-infragistics": "ng build --project @hyland/ui-infragistics --configuration production",
        ":build:stamp-versions": "gulp build:update-package-versions",
        ":build:update-package-name": "gulp build:update-package-name",
        ":build:update-icons-package-name-only": "gulp build:update-icons-package-name-only",
        ":build:translate-design-tokens": "node ./tools/build/palette-update.js",
        "ci": "npm-run-all :ci:**",
        ":ci:build:projects": "npm-run-all :build:project:** :build:stamp-versions :build:update-package-name :build:translate-design-tokens",
        ":ci:build:package-test": "npm-run-all :build:project:** :build:stamp-versions :build:update-icons-package-name-only :build:translate-design-tokens",
        ":ci:build:apps": "npm-run-all build:dev-app build:storybook",
        ":ci:test": "npm run test",
        ":ci:analyze-bundle": "npm run analyze-bundle -- --html test-results/bundle/index.html",
        "compodoc": "npm-run-all :compodoc:project:** --target=compodoc",
        ":compodoc:project:ui": "compodoc -t -c ./projects/hyland/ui/compodocrc.json",
        ":compodoc:project:ui-shell": "compodoc -t -c ./projects/hyland/ui-shell/compodocrc.json",
        ":compodoc:project:ui-infragistics": "compodoc -t -c ./projects/hyland/ui-infragistics/compodocrc.json",
        "simulate-packages": "npm run :ci:build:projects && cpx \"dist/hyland/**/*\" node_modules/@hyland",
        "hy-ui-docs": "cpx \"./projects/hyland/**/*.md\" dist/hyuidocs && cpx \"./projects/hyland/hy-ui-docs-config.json\" dist/hyuidocs",
        "prepare": "husky install"
    },
    "dependencies": {
        "@angular-eslint/builder": "18.3.1",
        "@angular/animations": "18.2.5",
        "@angular/cdk": "18.2.5",
        "@angular/common": "18.2.5",
        "@angular/compiler": "18.2.5",
        "@angular/core": "18.2.5",
        "@angular/forms": "18.2.5",
        "@angular/material": "18.2.5",
        "@angular/platform-browser": "18.2.5",
        "@angular/platform-browser-dynamic": "18.2.5",
        "@angular/router": "18.2.5",
        "@hylandsoftware/design-tokens": "^2.0.0-rc.2",
        "@hylandsoftware/hy-ui-icons": "^4.1.1",
        "@igniteui/material-icons-extended": "^3.0.2",
        "@jsverse/transloco": "^7.4.0",
        "@infragistics/igniteui-angular": "npm:igniteui-angular@18.1.13",
        "@juggle/resize-observer": "^3.4.0",
        "@storybook/core-server": "^8.2.9",
        "angular-imask": "^7.6.1",
        "angular-oauth2-oidc": "^18.0.0",
        "angular-split": "18.0.0-beta.1",
        "hammerjs": "^2.0.8",
        "igniteui-theming": "11.0.0",
        "ignore-not-found-export-webpack-plugin": "^1.0.2",
        "ngx-color": "^9.0.0",
        "rxjs": "^7.8.1",
        "shelljs-exec-proxy": "^0.2.1",
        "tslib": "^2.7.0",
        "zone.js": "^0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/architect": "0.1802.5",
        "@angular-devkit/build-angular": "18.2.5",
        "@angular-devkit/core": "18.2.5",
        "@angular-eslint/eslint-plugin": "18.3.1",
        "@angular-eslint/eslint-plugin-template": "18.3.1",
        "@angular-eslint/template-parser": "18.3.1",
        "@angular/cli": "18.2.5",
        "@angular/compiler-cli": "18.2.5",
        "@angular/elements": "18.2.5",
        "@angular/language-service": "18.2.5",
        "@commitlint/cli": "19.4.1",
        "@commitlint/config-conventional": "19.4.1",
        "@commitlint/prompt-cli": "19.4.1",
        "@compodoc/compodoc": "^1.1.25",
        "@cypress/code-coverage": "^3.13.2",
        "@cypress/grep": "^4.1.0",
        "@cypress/schematic": "^2.5.2",
        "@hylandsoftware/eslint-plugin-hy-ui-standards": "^7.0.0-rc.3",
        "@hylandsoftware/hy-ui-devkit": "^7.0.0",
        "@hylandsoftware/hy-ui-standards": "^7.0.0-rc.3",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
        "@storybook/addon-a11y": "^8.2.9",
        "@storybook/addon-actions": "^8.2.9",
        "@storybook/addon-docs": "^8.2.9",
        "@storybook/addon-essentials": "^8.2.9",
        "@storybook/addon-themes": "^8.2.9",
        "@storybook/addon-viewport": "^8.2.9",
        "@storybook/angular": "^8.2.9",
        "@storybook/channels": "^8.2.9",
        "@storybook/cli": "^8.2.9",
        "@storybook/components": "^8.2.9",
        "@storybook/manager-api": "^8.2.9",
        "@storybook/preview-api": "^8.2.9",
        "@storybook/theming": "^8.2.9",
        "@types/gulp": "^4.0.17",
        "@types/hammerjs": "^2.0.45",
        "@types/node": "22.5.4",
        "@types/react": "^18.3.5",
        "@typescript-eslint/eslint-plugin": "^8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "accessibility-insights-scan": "^3.0.0",
        "angular-http-server": "^1.12.0",
        "axe-core": "~4.10.0",
        "backstopjs": "6.3.25",
        "core-js": "^3.38.1",
        "cpx2": "^7.0.1",
        "cypress": "13.14.2",
        "cypress-axe": "^1.5.0",
        "cypress-fail-fast": "^7.1.1",
        "cypress-file-upload": "^5.0.8",
        "cypress-mochawesome-reporter": "^3.8.2",
        "cypress-multi-reporters": "^1.6.4",
        "cypress-plugin-tab": "^1.0.5",
        "cypress-real-events": "^1.13.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-stylelint": "^22.0.0",
        "eslint-plugin-import": "2.30.0",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-storybook": "^0.8.0",
        "gulp": "~5.0.0",
        "gulp-replace": "^1.1.4",
        "husky": "^8.0.3",
        "istanbul-lib-coverage": "^3.2.2",
        "lint-staged": "^15.2.10",
        "mocha-junit-reporter": "^2.2.1",
        "ng-packagr": "18.2.1",
        "npm-audit-resolver": "^3.0.0-RC.0",
        "npm-run-all": "~4.1.5",
        "nyc": "^17.0.0",
        "prettier": "^3.3.3",
        "pretty-quick": "^4.0.0",
        "shelljs": "^0.8.5",
        "source-map-explorer": "^2.5.3",
        "standard-version": "^9.5.0",
        "storybook": "^8.2.9",
        "storybook-addon-rtl": "^1.0.1",
        "style-loader": "^4.0.0",
        "stylelint-config-prettier": "^9.0.5",
        "terser-webpack-plugin": "^5.3.10",
        "ts-node": "10.9.2",
        "typescript": "~5.5.4",
        "wait-on": "^8.0.0",
        "webpack": "^5.94.0"
    },
    "private": true,
    "homepage": "https://ui.hyland.io/",
    "repository": {
        "type": "git",
        "url": "https://github.com/HylandSoftware/hy-ui"
    },
    "bugs": {
        "url": "https://jira.hyland.com/projects/NG/issues"
    },
    "lint-staged": {
        "*.ts": "eslint",
        "*.scss": "stylelint"
    }
}