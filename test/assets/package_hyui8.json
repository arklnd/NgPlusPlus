    "@cypress/code-coverage": "^3.10.0",
    "@cypress/grep": "^4.0.1",
    "@cypress/schematic": "^2.2.0",
    "@hylandsoftware/eslint-plugin-hy-ui-standards": "^6.0.0",
    "@hylandsoftware/hy-ui-devkit": "^6.0.0",
    "@hylandsoftware/hy-ui-standards": "^6.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
